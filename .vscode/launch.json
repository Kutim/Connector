{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "provider",
      "request": "launch",
      "mainClass": "org.eclipse.edc.boot.system.runtime.BaseRuntime",
      "classPaths": [
        "${workspaceFolder}/launchers/connector/build/libs/connector.jar"
      ],
      "projectName": "launchers",
      "cwd": "${workspaceFolder}",
      "env": {
        "EDC_FS_CONFIG": "${workspaceFolder}/launchers/connector/provider.properties"
      }
    },
    {
      "type": "java",
      "name": "consumer",
      "request": "launch",
      "mainClass": "org.eclipse.edc.boot.system.runtime.BaseRuntime",
      "classPaths": [
        "${workspaceFolder}/launchers/connector/build/libs/connector.jar"
      ],
      "projectName": "launchers",
      "cwd": "${workspaceFolder}",
      "env": {
        "EDC_FS_CONFIG": "${workspaceFolder}/launchers/connector/consumer.properties"
      }
    }
  ]
}