          "version": "0.2.0",
          "configurations": [
                    {
                              "type": "java",
                              "name": "Current File",
                              "request": "launch",
                              "mainClass": "${file}"
                    },
                    {
                              "type": "java",
                              "name": "BaseRuntime",
                              "request": "launch",
                              "mainClass": "org.eclipse.edc.boot.system.runtime.BaseRuntime",
                              "projectName": "boot",
                              "cwd": "${workspaceFolder}",
                              "env": {
                                        "EDC_FS_CONFIG": "${workspaceFolder}/launchers/connector/provider.properties"
                              }
                    }
          ]
}